{
    "name": "lcv/combined-constraints-bundle",
    "type": "symfony-bundle",
    "description": "Provides custom combined constraints",
    "keywords": [
        "constraint",
        "constraints"
    ],
    "homepage": "https://github.com/lcavero/LCVCombinedConstraintsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cavero Mart√≠nez",
            "email": "luiscaverodeveloper@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "lcv/common-exceptions": "^1.0",
        "symfony/form": "~4.3",
        "symfony/framework-bundle": "~4.3",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/validator": "~4.3",
        "ext-intl": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LCV\\CombinedConstraintsBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
